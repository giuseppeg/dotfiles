[alias]
	st = status
	ci = commit
[user]
	name = Giuseppe Gurgone
	email = giuseppeg@users.noreply.github.com
[core]
	editor = "subl -n -w"
	excludesfile = ~/.gitignore_global
[credential]
	helper = osxkeychain
[help]
  autocorrect = 1
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
